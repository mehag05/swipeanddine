{
  "expo": {
    "name": "swipe-and-dine",
    "slug": "swipe-and-dine",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "entryPoint": "./index.js",
    "userInterfaceStyle": "automatic",
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "newArchEnabled": true
  }
}
